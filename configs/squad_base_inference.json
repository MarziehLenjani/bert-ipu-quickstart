{
    "task": "SQUAD",
    "layers_per_ipu": 6,
    "num_layers": 12,
    "hidden_size": 768,
    "attention_heads": 12,
    "sequence_length": 128,
    "popart_dtype": "FLOAT16",
    "no_dropout": true,
    "loss_scaling": 1.0,
    "stochastic_rounding": true,
    "custom_ops": [
        "gather",
        "attention"
    ],
    "batches_per_step": 16,
    "epochs": 0.01,
    "epochs_per_save": 1,
    "input_files": [
        "data/squad/dev-v1.1.json"
    ],
    "vocab_file": "data/ckpts/uncased_L-12_H-768_A-12/vocab.txt",
    "squad_results_dir": "data/squad/results",
    "squad_evaluate_script": "data/squad/evaluate-v1.1.py",
    "shuffle": false,
    "pipeline": true,
    "inference": true,
    "squeeze_model": true,
    "low_latency_inference": true,
    "max_copy_merge_size": -1
}
